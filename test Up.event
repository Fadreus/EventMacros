automacro Up {
    run-once 1
	ConfigKeyNot $desired_lockMap
	exclusive 1
	BaseLevel 12..98
    call {
		switch ($.lvl) {
		    case (= 12..25) {
				do iconf Erva Azul 7 1 0
				$mapa{lockMap} = pay_fild08
				$mapa{city} = payon
		    }
		    case (= 26..40) {
				do iconf Unha Apodrecida 10 1 0
				do iconf Osso 10 1 0
				do iconf Mandíbula Horrenda 10 1 0
				do mconf 1084 0 0 0 #cugomelo negro, troço inútil
				$mapa{lockMap} = pay_dun00
				$mapa{city} = payon
		    }
		    case (= 41..45) {
				$mapa{lockMap} = pay_fild09
				$mapa{city} = payon
		    }
			case (= 46..53) {
				do iconf Erva Azul 7 1 0
				if (&invamount(Erva Azul) >= 7) {
					$mapa{lockMap} = iz_dun00
					$mapa{city} = izlude
				} else {
					$mapa{lockMap} = cmd_fild02
					$mapa{city} = comodo
				}
			}
			case (= 54..59) {
				do mconf Hidra -1 0 0
				$mapa{lockMap} = iz_dun02
				$mapa{city} = izlude
			}
			case (= 60..70) {
				$mapa{lockMap} = moc_fild17
				$mapa{city} = morocc
			}
			case (= 71..79) {
				$mapa{lockMap} = mjolnir_11
				$mapa{city} = prontera
			}
			case (= 80..98) {
				$mapa{lockMap} = gef_fild06
				$mapa{city} = geffen
			}
		}
		do conf -f desired_lockMap $mapa{lockMap}
		if (&config(saveMap) = $mapa{city}) {
			do conf lockMap $mapa{lockMap}
		} else {
			log =============================
			log Vou salvar em $mapa{city}
			log =============================
			call SetSaveIn "$mapa{city}"
		}
    }
}
